{
	"extends": ["wesbos/typescript", "eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"overrides": [],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		"no-console": 2,
		"prettier/prettier": [
			"error",

			{
				"trailingComma": "es5",
				"singleQuote": true,
				"printWidth": 120,
				"useTabs": true,
				"tabWidth": 8
			}
		]
	}
}
